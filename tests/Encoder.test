Test network coding: encoder module (G2C3 and G2C4)

%inifile: omnetpp.ini
[General]
sim-time-limit = 10ns

**.clock.inter = 2ns

**.encoder1.generationSize = 2
**.encoder1.numCombinations = 3
**.encoder1.internalType = "HaecComm.MW.NetworkCoding.EncoderImplFlit"
**.encoder2.generationSize = 2
**.encoder2.numCombinations = 4
**.encoder2.internalType = "HaecComm.MW.NetworkCoding.EncoderImplFlit"

%file: test.ned
import HaecComm.Core.Clock;
import HaecComm.MW.NetworkCoding.Encoder;

network Test {
    parameters:
        bool useGlobalTransmissionIds = false;
    submodules:
        clock: Clock;
        encoder1: Encoder;
        encoder2: Encoder;
        testapp: EncoderTestApp;
    connections:
        testapp.in1 <-- encoder1.out;
        testapp.out1 --> encoder1.in;
        testapp.in2 <-- encoder2.out;
        testapp.out2 --> encoder2.in;
}

