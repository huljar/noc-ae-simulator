%description:
Test signal emissions of the clock module

%inifile: omnetpp.ini
[General]
sim-time-limit = 10ns

**.clock.inter = 2ns

%file: test.ned
import HaecComm.Core.Clock;
import HaecCommTest.ClockTestApp;

network Test {
    submodules:
        clock: Clock;
        testapp: ClockTestApp;
}

%contains-regex: stdout
Clock cycle 0
.*
Clock cycle 1
.*
Clock cycle 2
.*
Clock cycle 3
.*
Clock cycle 4
.*
Clock cycle 5

%not-contains: stdout
undisposed object:

%not-contains: stdout
-- check module destructor
